{
  "name": "n8n-nodes-random",
  "version": "1.0.0",
  "description": "A custom n8n node to generate random numbers using random.org",
  "keywords": [
    "n8n-community-node-package"
  ],
  "main": "dist/nodes/Random/Random.node.js",
  "scripts": {
    "build": "tsc && copyfiles -u 1 nodes/**/*.svg dist/",
    "dev": "tsc --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Random/Random.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "copyfiles": "^2.4.1",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "typescript": "^5.4.5"
  }
}